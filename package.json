{
  "name": "ide-backend",
  "version": "0.0.0",
  "scripts": {
    "start": "node api/server.js",
    "dev": "nodemon --exec node api/server.js",
    "test": "mocha --timeout 60000 --exit test/**/*.js",
    "husky-init": "node node_modules/husky/husky.js install",
    "prettier": "prettier --config .prettierrc --ignore-path .eslintignore --write"
  },
  "dependencies": {
    "@google-cloud/pubsub": "^2.1.0",
    "ajv": "^6.10.2",
    "ajv-errors": "^1.0.1",
    "axios": "^0.19.0",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "google-auth-library": "^5.10.0",
    "grpc": "^1.24.2",
    "http-errors": "~1.8.0",
    "http-status-codes": "^1.4.0",
    "mongoose": "^5.9.21",
    "underscore": "^1.9.2"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^6.1.0",
    "eslint-config-google": "^0.13.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "mocha": "^8.0.1",
    "nodemon": "^2.0.4",
    "prettier": "^2.4.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run prettier",
      "git add",
      "eslint --ignore-path .eslintignore --config .eslintrc.json"
    ]
  },
  "engines": {
    "node": "8 || 10",
    "npm": ">= 5",
    "yarn": ">= 1"
  },
  "nodemonConfig": {
    "ignore": [
      "test/",
      "**/*.spec.js",
      "docs/"
    ],
    "delay": 1000
  },
  "bugs": {
    "email": "ifaisalalam@ctfhub.io"
  }
}
